{
    "manifest_version": 3,
    "name": "Asaki Zuki - Rotate Screen Support",
    "description": "hmm",
    "version": "1.0.0",
    "version_name": "v1.0 beta",
    "permissions": [],
    "author": "vantrong2007vn@gmail.com",
    "action": {
        "default_icon": {
            "16": "./extension_icon.png",
            "24": "./extension_icon.png",
            "32": "./extension_icon.png"
        }
    },
    "icons": {
        "32": "./extension_icon.png",
        "72": "./extension_icon.png",
        "128": "./extension_icon.png",
        "512": "./extension_icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "rotateAssets.js"
            ]
        }
    ]
}